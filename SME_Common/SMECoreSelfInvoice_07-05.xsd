                    <xsd:element name="ApplicableCITradeTax" minOccurs="0" maxOccurs="unbounded">
                      <xsd:annotation>
                        <xsd:documentation xml:lang="en">
                          <ccts:UniqueID>UN01005924</ccts:UniqueID>
