                                          <xsd:documentation xml:lang="en">
                                            <ccts:UniqueID>UN01005585</ccts:UniqueID>
                                            <ccts:Acronym>BBIE</ccts:Acronym>
                                            <ccts:DictionaryEntryName>CI_ Referenced_ Document. Line. Identifier</ccts:DictionaryEntryName>
                                            <ccts:Definition>The unique identifier of a line in this CI referenced document.</ccts:Definition>
                                          </xsd:documentation>
                                          <xsd:documentation xml:lang="ja">
