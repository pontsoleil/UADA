<!-- ===== SpecifiedCIDocumentVersionType                              ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="SpecifiedCIDocumentVersionType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005475</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Document_ Version. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Document Version</ccts:Name>
        <ccts:Definition>A specific variant of a Cross Industry (CI) document.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>バージョンクラス</ccts:Name>
        <ccts:Definition>バージョンに関する情報のクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ID" type="udt:IDType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005476</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Document_ Version. Identification. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>ID</ccts:Name>
              <ccts:Definition>The unique identifier for this CI document version. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>バージョンID</ccts:Name>
              <ccts:Definition>取引プロセスのバージョン識別番号</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="IssueDateTime" type="udt:DateTimeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005478</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Document_ Version. Issue. Date Time</ccts:DictionaryEntryName>
              <ccts:Name>Issue Date Time</ccts:Name>
              <ccts:Definition>The date, time, date time or other date time value of issue of this CI document version. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>バージョン発行日</ccts:Name>
              <ccts:Definition>取引プロセスのバージョン発行日</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
