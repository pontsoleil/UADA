            <xsd:documentation xml:lang="ja">
              <ccts:Name>金融機関名</ccts:Name>
              <ccts:Definition>債権者金融機関の、文字で表現された名前。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="JapanFinancialInstitutionCommonID" type="udt:IDType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01011521</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Creditor_ Financial Institution. Japan Financial Institution Common_ Identification. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>Japan Financial Institution Common ID</ccts:Name>
              <ccts:Definition>The Japan Financial Institution Common identifier as assigned by the Japanese Bankers Association for this CI creditor financial institution. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>金融機関番号</ccts:Name>
              <ccts:Definition>債権者の金融機関番号（日本の場合）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SubDivisionBranchFinancialInstitution" type="ram:SubDivisionBranchFinancialInstitutionType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005428</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Creditor_ Financial Institution. Sub-Division. Branch_ Financial Institution</ccts:DictionaryEntryName>
              <ccts:Name>Branch</ccts:Name>
              <ccts:Definition>The sub-division (branch) financial institution for this CI creditor financial institution. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>金融機関／金融機関支店グループ</ccts:Name>
              <ccts:Definition>債権者金融機関の支店金融機関グループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== IncludedCIILSupplyChainTradeLineItemType                    ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="IncludedCIILSupplyChainTradeLineItemType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005988</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Details</ccts:DictionaryEntryName>
        <ccts:Name>CIIL Trade Line Item</ccts:Name>
        <ccts:Definition>A collection of Cross Industry Invoice Line (CIIL) information specific to an item being used or reported on for supply chain trade purposes.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>明細文書ヘッダクラス</ccts:Name>
        <ccts:Definition>インボイス文書へ組み込まれる明細文書ヘッダのアイテムのクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="AssociatedCIILDocumentLineDocument" type="ram:AssociatedCIILDocumentLineDocumentType">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005989</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Associated. CIIL_ Document Line_ Document</ccts:DictionaryEntryName>
              <ccts:Name>Document Line</ccts:Name>
              <ccts:Definition>The document line associated with this CIIL supply chain trade line item. [Occurence Min=1 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ／明細文書グループ</ccts:Name>
              <ccts:Definition>明細文書ヘッダの明細文書グループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILSupplyChainTradeAgreement" type="ram:SpecifiedCIILSupplyChainTradeAgreementType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005990</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Specified. CIIL_ Supply Chain_ Trade Agreement</ccts:DictionaryEntryName>
              <ccts:Name>Trade Agreement</ccts:Name>
              <ccts:Definition>The CIIL supply chain trade agreement specified for this CIIL supply chain trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ／契約グループ</ccts:Name>
              <ccts:Definition>明細文書ヘッダの契約に関するグループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILSupplyChainTradeDelivery" type="ram:SpecifiedCIILSupplyChainTradeDeliveryType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005991</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Specified. CIIL_ Supply Chain_ Trade Delivery</ccts:DictionaryEntryName>
              <ccts:Name>Trade Delivery</ccts:Name>
              <ccts:Definition>The CIIL supply chain trade delivery specified for this CIIL supply chain trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ／配送グループ</ccts:Name>
              <ccts:Definition>明細文書ヘッダの配送に関する情報からなるグループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILSupplyChainTradeSettlement" type="ram:SpecifiedCIILSupplyChainTradeSettlementType">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005992</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Specified. CIIL_ Supply Chain_ Trade Settlement</ccts:DictionaryEntryName>
              <ccts:Name>Trade Settlement</ccts:Name>
              <ccts:Definition>The CIIL supply chain trade settlement specified for this CIIL supply chain trade line item. [Occurence Min=1 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ／決済グループ</ccts:Name>
              <ccts:Definition>明細文書ヘッダの決済に関するグループ。 文書タイプが「統合文書」を指定する場合にこのグループは任意。「単一文書」を指定する場合はこのグループは実装しない</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SubordinateCIILBSubordinateTradeLineItem" type="ram:SubordinateCIILBSubordinateTradeLineItemType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01009669</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIL_ Supply Chain_ Trade Line Item. Subordinate. CIILB_ Subordinate_ Trade Line Item</ccts:DictionaryEntryName>
              <ccts:Name>Subordinate Trade Line Item</ccts:Name>
              <ccts:Definition>A CIILB trade line item subordinate to this supply chain trade line item. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ／明細行グループ</ccts:Name>
              <ccts:Definition>明細文書ヘッダの明細行に関するグループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== SubordinateCIILBSubordinateTradeLineItemType                ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="SubordinateCIILBSubordinateTradeLineItemType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01009647</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Details</ccts:DictionaryEntryName>
        <ccts:Name>CIILB Subordinate Trade Line Item</ccts:Name>
        <ccts:Definition>A collection of information specific to a Cross Industry Invoice Line Breakdown (CIILB) item being used or reported on for trade purposes.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>明細行クラス</ccts:Name>
        <ccts:Definition>明細行に関する情報からなるクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ID" type="udt:IDType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01009648</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Identification. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>ID</ccts:Name>
              <ccts:Definition>An identifier for this CIILB subordinate trade line item. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行ID</ccts:Name>
              <ccts:Definition>この文書の明細行に関する情報を特定するために付与した行ID。明細行をユニークに識別するために付番する場合は文書IDとの複合キーで明細行を特定する。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="CategoryCode" type="udt:CodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01014637</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Category. Code</ccts:DictionaryEntryName>
              <ccts:Name>Category Code</ccts:Name>
              <ccts:Definition>The code specifying the category of this CIILB subordinate trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行類型コード</ccts:Name>
              <ccts:Definition>この明細行の取引類型（資産譲渡、返金・追加請求、調整等）を識別するコード。 デフォルトは「資産譲渡」</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="RequestedResponseTypeCode" type="udt:CodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01015599</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Requested_ Response Type. Code</ccts:DictionaryEntryName>
              <ccts:Name>Requested Response Type Code</ccts:Name>
              <ccts:Definition>The code specifying the type of response requested for this CIILB subordinate trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行回答タイプコード</ccts:Name>
              <ccts:Definition>この文書の明細行に関する回答タイプを特定するコード デフォルトは「確認」</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ApplicableCITradeProduct" type="ram:ApplicableCITradeProductType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01010016</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Applicable. CI_ Trade_ Product</ccts:DictionaryEntryName>
              <ccts:Name>Applicable Product</ccts:Name>
              <ccts:Definition>A product applicable for this CIILB subordinate trade line item. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行／取引品目グループ</ccts:Name>
              <ccts:Definition>明細行の取引品目に関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILBSupplyChainTradeAgreement" type="ram:SpecifiedCIILBSupplyChainTradeAgreementType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01009649</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Specified. CIILB_ Supply Chain_ Trade Agreement</ccts:DictionaryEntryName>
              <ccts:Name>Trade Agreement</ccts:Name>
              <ccts:Definition>The supply chain trade agreement specified for this CIILB subordinate trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行／取引契約グループ</ccts:Name>
              <ccts:Definition>明細行の取引契約に関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILBSupplyChainTradeDelivery" type="ram:SpecifiedCIILBSupplyChainTradeDeliveryType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01009650</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Specified. CIILB_ Supply Chain_ Trade Delivery</ccts:DictionaryEntryName>
              <ccts:Name>Trade Delivery</ccts:Name>
              <ccts:Definition>The supply chain trade delivery specified for this CIILB subordinate trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行／配送グループ</ccts:Name>
              <ccts:Definition>明細行の配送に関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedCIILBSupplyChainTradeSettlement" type="ram:SpecifiedCIILBSupplyChainTradeSettlementType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01009651</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Specified. CIILB_ Supply Chain_ Trade Settlement</ccts:DictionaryEntryName>
              <ccts:Name>Trade Settlement</ccts:Name>
              <ccts:Definition>The supply chain trade settlement specified for this CIILB subordinate trade line item. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行／決裁グループ</ccts:Name>
              <ccts:Definition>明細行の決済に関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="IncludedCINote" type="ram:IncludedCINoteType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01015533</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIILB_ Subordinate_ Trade Line Item. Included. CI_ Note</ccts:DictionaryEntryName>
              <ccts:Name>Included Note</ccts:Name>
              <ccts:Definition>A note included in this CIILB subordinate trade line item. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行／注釈グループ</ccts:Name>
              <ccts:Definition>明細行の注釈に関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== SpecifiedCITradeSettlementPaymentMeansType                  ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="SpecifiedCITradeSettlementPaymentMeansType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005670</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Trade Settlement Payment Means</ccts:Name>
        <ccts:Definition>The means by which a payment will be or has been made for Cross Industry (CI) trade settlement purposes.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>支払手段クラス</ccts:Name>
        <ccts:Definition>取引決済の目的で支払が行われる、あるいは行われた手段のクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="TypeCode" type="qdt:PaymentMeansCodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005672</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Type. Code</ccts:DictionaryEntryName>
              <ccts:Name>Type Code</ccts:Name>
              <ccts:Definition>The code specifying the type of CI trade settlement payment means, such as cash or cheque. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払手段タイプコード</ccts:Name>
              <ccts:Definition>取引決済手段のタイプを識別するコード</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="Information" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01011456</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Information. Text</ccts:DictionaryEntryName>
              <ccts:Name>Information</ccts:Name>
              <ccts:Definition>Information, expressed as text, for this CI trade settlement payment means. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払手段情報</ccts:Name>
              <ccts:Definition>取引決済手段に関する情報</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="PaidAmount" type="udt:AmountType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01015504</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Paid. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Paid Amount</ccts:Name>
              <ccts:Definition>The monetary value paid or to be paid by this CI trade settlement payment means. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払金額</ccts:Name>
              <ccts:Definition>取引決済手段で支払う金額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="PayeePartyCICreditorFinancialAccount" type="ram:PayeePartyCICreditorFinancialAccountType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005677</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Payee_ Party. CI_ Creditor_ Financial Account</ccts:DictionaryEntryName>
              <ccts:Name>Payee Creditor Financial Account</ccts:Name>
              <ccts:Definition>The creditor financial account of the payee party for this CI trade settlement payment means. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払手段／金融口座グループ</ccts:Name>
              <ccts:Definition>取引決済の支払手段に関する受取人である当事者の債権者金融口座のグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="PayeeSpecifiedCICreditorFinancialInstitution" type="ram:PayeeSpecifiedCICreditorFinancialInstitutionType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005679</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Payee_ Specified. CI_ Creditor_ Financial Institution</ccts:DictionaryEntryName>
              <ccts:Name>Payee Creditor Financial Institution</ccts:Name>
              <ccts:Definition>The creditor financial institution of the payee party specified for this CI trade settlement payment means. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払手段／金融機関グループ</ccts:Name>
              <ccts:Definition>取引決済の支払手段に対して特定された受取人である当事者の債権者金融機関のグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ApplicableTradeSettlementFinancialCard" type="ram:ApplicableTradeSettlementFinancialCardType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01006057</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade Settlement_ Payment Means. Applicable. Trade Settlement_ Financial Card</ccts:DictionaryEntryName>
              <ccts:Name>Financial Card</ccts:Name>
              <ccts:Definition>The trade settlement financial card applicable to this CI trade settlement payment means. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払手段／金融カードグループ</ccts:Name>
              <ccts:Definition>支払手段の金融カードに関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
