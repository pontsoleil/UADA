              <ccts:Name>Transaction Processing Date Time</ccts:Name>
              <ccts:Definition>The date, time, date time, or other date time value of the processing of a transaction for this CI exchanged document context. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>処理日時</ccts:Name>
              <ccts:Definition>メッセージがやり取りされる日時</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="BusinessProcessSpecifiedCIDocumentContextParameter" type="ram:BusinessProcessSpecifiedCIDocumentContextParameterType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005481</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
