            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>取引設定内容/取引プロセスグループ</ccts:Name>
              <ccts:Definition>取引内容の取引プロセスに関する情報</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ScenarioSpecifiedCIDocumentContextParameter" type="ram:ScenarioSpecifiedCIDocumentContextParameterType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005483</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Exchanged Document_ Context. Scenario_ Specified. CI_ Document Context_ Parameter</ccts:DictionaryEntryName>
