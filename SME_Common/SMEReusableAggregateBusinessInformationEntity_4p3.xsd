  <xsd:complexType name="SpecifiedCIIHTradeSettlementMonetarySummationType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005941</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Details</ccts:DictionaryEntryName>
        <ccts:Name>CIIH Monetary Summation</ccts:Name>
        <ccts:Definition>A collection of trade settlement monetary totals specified for this Cross Industry Invoice Header (CIIH).</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>インボイス文書合計金額クラス</ccts:Name>
        <ccts:Definition>インボイス文書の鑑ヘッダ合計金額に関する情報からなるクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ChargeTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005943</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Charge Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Charge Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the total of all charge amounts being reported on header level in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ追加請求合計金額（税抜き）</ccts:Name>
              <ccts:Definition>明細文書ヘッダ追加請求合計金額（税抜き）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="AllowanceTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005944</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Allowance Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Allowance Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the total of all allowance amounts being reported on header level in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ返金合計金額（税抜き）</ccts:Name>
              <ccts:Definition>明細文書ヘッダ返金合計金額（税抜き）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="TaxTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005946</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Tax Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Tax Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the total of all tax amounts being reported in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ消費税総合計額</ccts:Name>
              <ccts:Definition>明細文書ヘッダ税クラスで算出した税率別消費税額の合計額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="GrandTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005948</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Grand Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Grand Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the grand total of this CIIH trade settlement monetary summation, including taxes and allowances and charges. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>インボイス文書総合計金額（税込み）</ccts:Name>
              <ccts:Definition>Σ明細文書ヘッダ譲渡資産合計金額（税抜き）＋鑑ヘッダ返金・追加請求合計金額（税抜き）＋Σ明細文書ヘッダ合計消費税額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="TotalPrepaidAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005950</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Total Prepaid. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Total Prepaid Amount</ccts:Name>
              <ccts:Definition>A monetary value of a prepaid total reported in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>前払合計金額</ccts:Name>
              <ccts:Definition>すでに前払いで支払済合計金額 Σ前払金額（税込み）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuePayableAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01008445</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Due Payable. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Due Payable Amount</ccts:Name>
              <ccts:Definition>A monetary value that is an amount due and payable for this CIIH trade settlement monetary summation, such as the amount due to the creditor. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払責務金額総合計</ccts:Name>
              <ccts:Definition>支払責務金額総合計＝Σ明細文書ヘッダ譲渡資産合計金額(税抜き)+明細文書ヘッダ消費税合計額＋明細文書ヘッダ課税対象外金額＋未決済合計金額ーΣ鑑ヘッダ調整金額ーΣ鑑ヘッダ調整税額（税率別）ー前払合計金額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="NetLineTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01008451</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Net_ Line Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Net Line Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the total of all line amounts, including line level allowances and charges and excluding line level taxes, being reported in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ譲渡資産合計金額(税抜き)</ccts:Name>
              <ccts:Definition>鑑ヘッダ譲渡資産金額総合計金額（税抜き）＝∑明細文書ヘッダ譲渡資産合計金額（税抜き）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="IncludingTaxesLineTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01013091</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Including Taxes_ Line Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Including Taxes Line Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the line total, including taxes, being reported in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ譲渡資産合計金額(税込み)</ccts:Name>
              <ccts:Definition>鑑ヘッダ譲渡資産金額総合計金額（税込み） '＝∑明細文書ヘッダ譲渡資産合計金額（税込み）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
