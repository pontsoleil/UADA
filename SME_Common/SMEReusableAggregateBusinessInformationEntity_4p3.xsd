              <ccts:Definition>A scenario CI document context parameter specified for this CI exchanged document context. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>取引設定内容/取引シナリオグループ</ccts:Name>
              <ccts:Definition>取引内容の取引シナリオに関する情報</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ApplicationSpecifiedCIDocumentContextParameter" type="ram:ApplicationSpecifiedCIDocumentContextParameterType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005484</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
