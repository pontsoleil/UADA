<!-- ======================================================================= -->
<!-- ===== Type Declarations                                           ===== -->
<!-- ======================================================================= -->
<!-- ======================================================================= -->
<!-- ===== ReceivableSpecifiedCITradeAccountingAccountType             ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="ReceivableSpecifiedCITradeAccountingAccountType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005680</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Trade_ Accounting Account. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Trade Accounting Account</ccts:Name>
        <ccts:Definition>A specific Cross Industry (CI) trade account for recording debits and credits to general accounting, cost accounting or budget accounting.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>会計アカウントクラス</ccts:Name>
        <ccts:Definition>会計アカウントに関するクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ID" type="udt:IDType">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005681</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Accounting Account. Identification. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>ID</ccts:Name>
              <ccts:Definition>The unique identifier for this CI trade accounting account. [Occurence Min=1 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ会計アカウントID</ccts:Name>
              <ccts:Definition>明細文書ヘッダ会計アカウントのID</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="TypeCode" type="qdt:AccountingAccountTypeCodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005683</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Accounting Account. Type. Code</ccts:DictionaryEntryName>
              <ccts:Name>Type Code</ccts:Name>
              <ccts:Definition>The code specifying the type of CI trade accounting account, such as general (main), secondary, cost accounting or budget account. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ会計アカウントタイプコード</ccts:Name>
              <ccts:Definition>明細文書ヘッダ会計アカウントのタイプコード</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="Name" type="udt:TextType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005685</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Accounting Account. Name. Text</ccts:DictionaryEntryName>
              <ccts:Name>Name</ccts:Name>
              <ccts:Definition>The name, expressed as text, of this CI trade accounting account. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細文書ヘッダ会計アカウント名</ccts:Name>
              <ccts:Definition>明細文書ヘッダ会計アカウントの名称</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== PayeePartyCICreditorFinancialAccountType                    ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="PayeePartyCICreditorFinancialAccountType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005398</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Creditor_ Financial Account. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Creditor Financial Account</ccts:Name>
        <ccts:Definition>A specific Cross Industry (CI) business arrangement whereby credits arising from transactions are recorded.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>金融口座クラス</ccts:Name>
        <ccts:Definition>債権者の銀行口座のクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="AccountName" type="udt:TextType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005400</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Creditor_ Financial Account. Account Name. Text</ccts:DictionaryEntryName>
              <ccts:Name>Name</ccts:Name>
              <ccts:Definition>The account name, expressed as text, of this CI creditor financial account. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>口座名義</ccts:Name>
              <ccts:Definition>債権者金融口座の、文字で表現された口座名。 半角カナ（日本の場合）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ProprietaryID" type="udt:IDType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005401</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Creditor_ Financial Account. Proprietary_ Identification. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>Proprietary ID</ccts:Name>
              <ccts:Definition>The unique proprietary identifier for this CI creditor financial account. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>口座番号</ccts:Name>
              <ccts:Definition>債権者金融口座の一意の所有者ID。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="TypeCode" type="udt:CodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01012127</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Creditor_ Financial Account. Type. Code</ccts:DictionaryEntryName>
              <ccts:Name>Type Code</ccts:Name>
              <ccts:Definition>The code specifying the type of financial account for this CI creditor. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>口座種別コード</ccts:Name>
              <ccts:Definition>債権者金融口座種別のコード。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== PostalCITradeAddressType                                    ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="PostalCITradeAddressType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005687</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Trade_ Address. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Trade Address</ccts:Name>
        <ccts:Definition>The place at which a particular Cross Industry (CI) trade related organization or person may be found or reached.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>受注者住所クラス</ccts:Name>
        <ccts:Definition>受注者住所に関する情報からなるクラス。</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="PostcodeCode" type="udt:CodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005689</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Address. Postcode. Code</ccts:DictionaryEntryName>
              <ccts:Name>Postcode</ccts:Name>
              <ccts:Definition>The postcode of this CI trade address. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>受注者郵便番号</ccts:Name>
              <ccts:Definition>受注者の郵便番号。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="LineOne" type="udt:TextType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005692</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Address. Line One. Text</ccts:DictionaryEntryName>
              <ccts:Name>Line One</ccts:Name>
              <ccts:Definition>The first free form line, expressed as text, of this CI trade address. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>受注者住所1</ccts:Name>
              <ccts:Definition>受注者の住所1行目。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="LineTwo" type="udt:TextType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005693</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Address. Line Two. Text</ccts:DictionaryEntryName>
              <ccts:Name>Line Two</ccts:Name>
              <ccts:Definition>The second free form line, expressed as text, of this CI trade address. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>受注者住所2</ccts:Name>
              <ccts:Definition>受注者の住所2行目。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="LineThree" type="udt:TextType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005694</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Address. Line Three. Text</ccts:DictionaryEntryName>
              <ccts:Name>Line Three</ccts:Name>
              <ccts:Definition>The third free form line, expressed as text, of this CI trade address. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>受注者住所3</ccts:Name>
              <ccts:Definition>受注者の住所3行目。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="CountryID" type="qdt:CountryIDType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005700</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Trade_ Address. Country. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>Country Code</ccts:Name>
              <ccts:Definition>The unique identifier of the country for this CI trade address. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>受注者国ID</ccts:Name>
              <ccts:Definition>受注者の国ID。デフォルトは「JP」</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== SpecifiedCIFinancialAdjustmentType                          ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="SpecifiedCIFinancialAdjustmentType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005487</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Financial Adjustment</ccts:Name>
        <ccts:Definition>A correction or modification to reflect actual Cross Industry (CI) financial conditions.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>鑑ヘッダ調整（修正差額調整）クラス</ccts:Name>
        <ccts:Definition>鑑ヘッダ調整に関する情報からなるクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ReasonCode" type="qdt:FinancialAdjustmentReasonCodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005488</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Reason. Code</ccts:DictionaryEntryName>
              <ccts:Name>Reason Code</ccts:Name>
              <ccts:Definition>The code specifying a reason for this CI financial adjustment. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ調整理由コード（修正差額調整）</ccts:Name>
              <ccts:Definition>鑑ヘッダ調整金額の内容を識別するコード=62（修正差額調整：デフォルト）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="Reason" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005489</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Reason. Text</ccts:DictionaryEntryName>
              <ccts:Name>Reason Text</ccts:Name>
              <ccts:Definition>A reason, expressed as text, for this CI financial adjustment. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ調整理由（修正差額調整）</ccts:Name>
              <ccts:Definition>鑑ヘッダ調整の内容説明</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ActualAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005490</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Actual. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Actual Amount</ccts:Name>
              <ccts:Definition>An actual monetary value added or subtracted as a result of this CI financial adjustment. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ調整金額（修正差額調整合計額）</ccts:Name>
              <ccts:Definition>＜鑑ヘッダ調整理由コード=62（デフォルト）＞鑑ヘッダ調整金額（鑑ヘッダで減額処理） = 前回インボイス金額ー修正インボイス金額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ActualDateTime" type="udt:DateTimeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005492</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Actual. Date Time</ccts:DictionaryEntryName>
              <ccts:Name>Actual Date Time</ccts:Name>
              <ccts:Definition>The actual date, time, date time, or other date time value of this CI financial adjustment. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>明細行調整実施日</ccts:Name>
              <ccts:Definition>この明細行の調整実施日</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="DirectionCode" type="qdt:AccountingDebitCreditStatusCodeType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01014649</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Direction. Code</ccts:DictionaryEntryName>
              <ccts:Name>Direction Code</ccts:Name>
              <ccts:Definition>The code specifying whether the CI financial adjustment is an addition or a subtraction. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ調整取引方向コード</ccts:Name>
              <ccts:Definition>鑑ヘッダ調整額の＋ーを識別するコード マイナス金額を許容しない場合は必須</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="RelatedCITradeTax" type="ram:RelatedCITradeTaxType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01014897</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Financial_ Adjustment. Related. CI_ Trade_ Tax</ccts:DictionaryEntryName>
              <ccts:Name>Related Tax</ccts:Name>
              <ccts:Definition>A trade tax related to this CI financial adjustment. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>鑑ヘッダ調整／調整税グループ</ccts:Name>
              <ccts:Definition>鑑ヘッダ調整の税率別税クラスに関するグループ</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- ======================================================================= -->
<!-- ===== ApplicableCIIHSupplyChainTradeAgreementType                 ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="ApplicableCIIHSupplyChainTradeAgreementType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005878</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CIIH_ Supply Chain_ Trade Agreement. Details</ccts:DictionaryEntryName>
        <ccts:Name>CIIH Trade Agreement</ccts:Name>
        <ccts:Definition>The contractual terms of the supply chain trade agreement for this Cross Industry Invoice Header (CIIH).</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>インボイス文書契約クラス</ccts:Name>
        <ccts:Definition>インボイス文書に関する契約に関する情報からなるクラス。</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="SellerCITradeParty" type="ram:SellerCITradePartyType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005879</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Supply Chain_ Trade Agreement. Seller. CI_ Trade_ Party</ccts:DictionaryEntryName>
              <ccts:Name>Seller</ccts:Name>
              <ccts:Definition>The party that is the seller for this CIIH supply chain trade agreement. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>インボイス文書契約／受注者グループ</ccts:Name>
              <ccts:Definition>受注者に関するグループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="BuyerCITradeParty" type="ram:BuyerCITradePartyType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005880</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Supply Chain_ Trade Agreement. Buyer. CI_ Trade_ Party</ccts:DictionaryEntryName>
              <ccts:Name>Buyer</ccts:Name>
              <ccts:Definition>The party that is the buyer for this CIIH supply chain trade agreement. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>インボイス文書契約／発注者グループ</ccts:Name>
              <ccts:Definition>発注者に関するグループ。</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="SpecifiedProcuringProject" type="ram:SpecifiedProcuringProjectType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01011516</ccts:UniqueID>
              <ccts:Acronym>ASBIE</ccts:Acronym>
