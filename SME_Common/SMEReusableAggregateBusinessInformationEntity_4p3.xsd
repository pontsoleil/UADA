<!-- ===== OutstandingSpecifiedCIIHTradeSettlementMonetarySummationType ===== -->
<!-- ======================================================================= -->
  <xsd:complexType name="OutstandingSpecifiedCIIHTradeSettlementMonetarySummationType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005941</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Details</ccts:DictionaryEntryName>
        <ccts:Name>CIIH Monetary Summation</ccts:Name>
        <ccts:Definition>A collection of trade settlement monetary totals specified for this Cross Industry Invoice Header (CIIH).</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>未決済合計金額クラス</ccts:Name>
        <ccts:Definition>未決済合計金額に関する情報からなるクラス</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="GrandTotalAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005948</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Grand Total. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Grand Total Amount</ccts:Name>
              <ccts:Definition>A monetary value of the grand total of this CIIH trade settlement monetary summation, including taxes and allowances and charges. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>前回インボイス総合計金額（税込み）</ccts:Name>
              <ccts:Definition>前回インボイス文書の総合計金額（税込み）</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="TotalPrepaidAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005950</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Total Prepaid. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Total Prepaid Amount</ccts:Name>
              <ccts:Definition>A monetary value of a prepaid total reported in this CIIH trade settlement monetary summation. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>支払済金額（前回インボイス分）</ccts:Name>
              <ccts:Definition>前回インボイス文書総合計金額のうち、支払済合計金額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="DuePayableAmount" type="udt:AmountType" minOccurs="0" maxOccurs="unbounded">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01008445</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CIIH_ Trade Settlement_ Monetary Summation. Due Payable. Amount</ccts:DictionaryEntryName>
              <ccts:Name>Due Payable Amount</ccts:Name>
              <ccts:Definition>A monetary value that is an amount due and payable for this CIIH trade settlement monetary summation, such as the amount due to the creditor. [Occurence Min=0 Max=unbounded]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>未決済今回支払金額</ccts:Name>
              <ccts:Definition>未決済繰越金額に関する今回支払金額</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
