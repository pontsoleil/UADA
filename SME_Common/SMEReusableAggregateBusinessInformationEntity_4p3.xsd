  <xsd:complexType name="CIExchangedDocumentContextType">
    <xsd:annotation>
      <xsd:documentation xml:lang="en">
        <ccts:UniqueID>UN01005479</ccts:UniqueID>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>CI_ Exchanged Document_ Context. Details</ccts:DictionaryEntryName>
        <ccts:Name>CI Document Context</ccts:Name>
        <ccts:Definition>The scenario or setting of a Cross Industry (CI) exchanged document, such as its business process application context.</ccts:Definition>
      </xsd:documentation>
      <xsd:documentation xml:lang="ja">
        <ccts:Name>取引設定クラス</ccts:Name>
        <ccts:Definition>メッセージの内容に関する情報</ccts:Definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="SpecifiedTransactionID" type="udt:IDType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation xml:lang="en">
              <ccts:UniqueID>UN01005480</ccts:UniqueID>
              <ccts:Acronym>BBIE</ccts:Acronym>
              <ccts:DictionaryEntryName>CI_ Exchanged Document_ Context. Specified_ Transaction. Identifier</ccts:DictionaryEntryName>
              <ccts:Name>Transaction ID</ccts:Name>
              <ccts:Definition>The identifier of a specified transaction in this CI exchanged document context. [Occurence Min=0 Max=1]</ccts:Definition>
            </xsd:documentation>
            <xsd:documentation xml:lang="ja">
              <ccts:Name>取引ID</ccts:Name>
              <ccts:Definition>メッセージがやり取りされる取引識別番号</ccts:Definition>
            </xsd:documentation>
          </xsd:annotation>
      </xsd:element>
      <xsd:element name="ProcessingTransactionDateTime" type="udt:DateTimeType" minOccurs="0">
