          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CalculatedRate" type="udt:RateType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <ccts:UniqueID>UN01005836</ccts:UniqueID>
            <ccts:Acronym>BBIE</ccts:Acronym>
