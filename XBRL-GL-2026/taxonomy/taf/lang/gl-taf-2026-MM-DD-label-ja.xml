        <loc xlink:type="locator" xlink:href="../gl-taf-2026-MM-DD.xsd#gl-taf_detailOriginatingDocumentDate" xlink:label="detailOriginatingDocumentDate"/>
        <label xlink:type="resource" xlink:label="detailOriginatingDocumentDate_lbl" xlink:role="http://www.xbrl.org/2003/role/label" xlink:title="gl-taf_detailOriginatingDocumentDate_ja" xml:lang="en">発生元文書日付</label>
        <label xlink:type="resource" xlink:label="detailOriginatingDocumentDate_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の日付。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="detailOriginatingDocumentDate" xlink:to="detailOriginatingDocumentDate_lbl"/>
        <!-- gl-taf:detailOriginatingDocumentIdentifierType 発生元文書対象CVE種別 -->
        <loc xlink:type="locator" xlink:href="../gl-taf-2026-MM-DD.xsd#gl-taf_detailOriginatingDocumentIdentifierType" xlink:label="detailOriginatingDocumentIdentifierType"/>
