        <label xlink:type="resource" xlink:label="detailOriginatingDocumentNumber_lbl" xlink:role="http://www.xbrl.org/2003/role/documentation" xml:lang="ja">発生元文書の番号。</label>
        <labelArc xlink:type="arc" xlink:arcrole="http://www.xbrl.org/2003/arcrole/concept-label" xlink:from="detailOriginatingDocumentNumber" xlink:to="detailOriginatingDocumentNumber_lbl"/>
