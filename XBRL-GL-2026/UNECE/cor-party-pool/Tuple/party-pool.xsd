<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://example.org/xbrl/gl/cor" elementFormDefault="qualified" attributeFormDefault="unqualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xbrli="http://www.xbrl.org/2003/instance" xmlns:cor="http://example.org/xbrl/gl/cor">
	<import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
	<!-- Child items / tuples (simplified) -->
	<element name="PartyName" type="xbrli:stringItemType" substitutionGroup="xbrli:item" xbrli:periodType="instant" abstract="false"/>
	<element name="Address" type="xbrli:stringItemType" substitutionGroup="xbrli:item" xbrli:periodType="instant" abstract="false"/>
	<element name="Contact" type="xbrli:stringItemType" substitutionGroup="xbrli:item" xbrli:periodType="instant" abstract="false"/>
	<element name="TaxRegistration" type="xbrli:stringItemType" substitutionGroup="xbrli:item" xbrli:periodType="instant" abstract="false"/>
	<element name="AuthorityRegisteredIdentifier" type="xbrli:stringItemType" substitutionGroup="xbrli:item" xbrli:periodType="instant" abstract="false"/>
	<!-- Party tuple type in the pool -->
	<complexType name="PartyType">
		<sequence>
			<element ref="cor:PartyName" minOccurs="0" maxOccurs="1"/>
			<element ref="cor:Address" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="cor:Contact" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="cor:TaxRegistration" minOccurs="0" maxOccurs="unbounded"/>
			<element ref="cor:AuthorityRegisteredIdentifier" minOccurs="0" maxOccurs="unbounded"/>
		</sequence>
	</complexType>
	<element name="Party" type="cor:PartyType" substitutionGroup="xbrli:tuple" nillable="false"/>
</schema>
