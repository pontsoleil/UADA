                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/hypercube-dimension"
                        xlink:from="h_document_line" xlink:to="d_document_line"
                        xlink:title="hypercube-dimension: h_document_line to d_document_line"
                        order="1"/>

    <!-- dimension-domain -->

    <!-- domain-member -->
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineID" xlink:label="cor_DocumentLineID"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_document_line" xlink:to="cor_DocumentLineID"
                        order="1"/>

    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCode" xlink:label="cor_DocumentLineStatusCode"/>
    <link:definitionArc xlink:type="arc" 
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="v_document_line" xlink:to="cor_DocumentLineStatusCode" 
                        order="2"/>
  </link:definitionLink>

  <link:definitionLink xlink:type="extended" xlink:role="http://example.org/xbrl/gl/cor/role/codelist-document-type-code">
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentTypeCodeDomain" xlink:label="cor_DocumentTypeCodeDomain"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentTypeCodeValue_Invoice" xlink:label="cor_DocumentTypeCodeValue_Invoice"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentTypeCodeValue_CreditNote" xlink:label="cor_DocumentTypeCodeValue_CreditNote"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentTypeCodeValue_Order" xlink:label="cor_DocumentTypeCodeValue_Order"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentTypeCodeValue_Other" xlink:label="cor_DocumentTypeCodeValue_Other"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentTypeCodeDomain" xlink:to="cor_DocumentTypeCodeValue_Invoice" order="1" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentTypeCodeDomain" xlink:to="cor_DocumentTypeCodeValue_CreditNote" order="2" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentTypeCodeDomain" xlink:to="cor_DocumentTypeCodeValue_Order" order="3" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentTypeCodeDomain" xlink:to="cor_DocumentTypeCodeValue_Other" order="4" xbrldt:usable="true"/>
  </link:definitionLink>

  <link:definitionLink xlink:type="extended" xlink:role="http://example.org/xbrl/gl/cor/role/codelist-document-line-status-code">
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCodeDomain" xlink:label="cor_DocumentLineStatusCodeDomain"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCodeValue_Open" xlink:label="cor_DocumentLineStatusCodeValue_Open"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCodeValue_Closed" xlink:label="cor_DocumentLineStatusCodeValue_Closed"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCodeValue_Cancelled" xlink:label="cor_DocumentLineStatusCodeValue_Cancelled"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_DocumentLineStatusCodeValue_Other" xlink:label="cor_DocumentLineStatusCodeValue_Other"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentLineStatusCodeDomain" xlink:to="cor_DocumentLineStatusCodeValue_Open" order="1" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentLineStatusCodeDomain" xlink:to="cor_DocumentLineStatusCodeValue_Closed" order="2" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentLineStatusCodeDomain" xlink:to="cor_DocumentLineStatusCodeValue_Cancelled" order="3" xbrldt:usable="true"/>
    <link:definitionArc xlink:type="arc" xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member" 
                        xlink:from="cor_DocumentLineStatusCodeDomain" xlink:to="cor_DocumentLineStatusCodeValue_Other" order="4" xbrldt:usable="true"/>
  </link:definitionLink>
</link:linkbase>
