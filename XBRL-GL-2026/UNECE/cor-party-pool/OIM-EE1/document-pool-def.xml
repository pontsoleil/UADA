                        xlink:title="domain-member: v_document to p_document_line in pool-document-line"
                        order="5"/>
  </link:definitionLink>

  <link:definitionLink xlink:type="extended" xlink:role="http://example.org/xbrl/gl/cor/role/pool-party">
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#p_party" xlink:label="p_party"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#h_party" xlink:label="h_party"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#d_party" xlink:label="d_party"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#v_party" xlink:label="v_party"/>

    <!-- p_party all (has-hypercube) h_party link_party -->
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/all"
                        xlink:from="p_party" xlink:to="h_party"
                        xlink:title="all (has-hypercube): p_party to h_party"
                        order="1"
                        xbrldt:closed="true"
                        xbrldt:contextElement="segment"/>

    <!-- hypercube-dimension -->
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/hypercube-dimension"
                        xlink:from="h_party" xlink:to="d_party"
                        xlink:title="hypercube-dimension: h_party to d_party"
                        order="1"/>

    <!-- dimension-domain -->

    <!-- domain-member -->
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_PartyName" xlink:label="cor_PartyName"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_party" xlink:to="cor_PartyName"
                        order="1"/>

    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_Address" xlink:label="cor_Address"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_party" xlink:to="cor_Address"
                        order="2"/>

    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_Contact" xlink:label="cor_Contact"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_party" xlink:to="cor_Contact"
                        order="3"/>

    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_TaxRegistration" xlink:label="cor_TaxRegistration"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_party" xlink:to="cor_TaxRegistration"
                        order="4"/>

    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#cor_AuthorityRegisteredIdentifier" xlink:label="cor_AuthorityRegisteredIdentifier"/>
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/domain-member"
                        xlink:from="v_party" xlink:to="cor_AuthorityRegisteredIdentifier"
                        order="5"/>
  </link:definitionLink>

  <link:definitionLink xlink:type="extended" xlink:role="http://example.org/xbrl/gl/cor/role/pool-document-line">
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#p_document_line" xlink:label="p_document_line"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#h_document_line" xlink:label="h_document_line"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#d_document_line" xlink:label="d_document_line"/>
    <link:loc xlink:type="locator" xlink:href="document-pool-oim.xsd#v_document_line" xlink:label="v_document_line"/>

    <!-- p_document_line all (has-hypercube) h_document_line link_document_line -->
    <link:definitionArc xlink:type="arc"
                        xlink:arcrole="http://xbrl.org/int/dim/arcrole/all"
                        xlink:from="p_document_line" xlink:to="h_document_line"
                        xlink:title="all (has-hypercube): p_document_line to h_document_line"
                        order="1"
